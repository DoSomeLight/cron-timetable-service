{
	"compilerOptions": {
		"types": ["express"],
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "esnext",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"declaration": false,
		"importHelpers": true,
		"typeRoots": ["node_modules/@types"],
		"lib": ["es2021", "dom"],
		"skipLibCheck": true,
		"skipDefaultLibCheck": true
	},
	"lib": ["es2021"],
	"exclude": ["views/", "dist/"]
}
